<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Geo-location Test Harness</title>
    <link href="http://ss-cdn.trafficmanager.net/Content/bootstrap.css" rel="stylesheet" />
    <link href="http://ss-cdn.trafficmanager.net/Content/quest.css" rel="stylesheet" />
    <link href="http://ss-cdn.trafficmanager.net/Content/quest1.css" rel="stylesheet" />
    <link href="http://ss-cdn.trafficmanager.net/Content/quest2.css" rel="stylesheet" />
    <link href="http://ss-cdn.trafficmanager.net/Content/quest3.css" rel="stylesheet" />
    <link href="http://ss-cdn.trafficmanager.net/Content/Site.css" rel="stylesheet" />
    <script src="http://ss-cdn.trafficmanager.net/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Geo-location Test Harness", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Geo-location Test Harness</p>
        </footer>
    </div>
    <script src="http://ss-cdn.trafficmanager.net/Scripts/jquery-1.10.2.min.js"></script>
    <script src="http://ss-cdn.trafficmanager.net/Scripts/jquery.validate.min.js"></script>
    <script src="http://ss-cdn.trafficmanager.net/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="http://ss-cdn.trafficmanager.net/Scripts/bootstrap.min.js"></script>
    <script src="http://ss-cdn.trafficmanager.net/Scripts/quest1.js"></script>
    <script src="http://ss-cdn.trafficmanager.net/Scripts/quest2.js"></script>
    <script src="http://ss-cdn.trafficmanager.net/Scripts/quest3.js"></script>
    <script src="http://ss-cdn.trafficmanager.net/Scripts/quest4.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>